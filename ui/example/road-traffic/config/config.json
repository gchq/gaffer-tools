{
  "title": "Road Traffic Example",
  "operations": {
    "defaultAvailable": [
      {
        "name": "Get Elements",
        "class": "uk.gov.gchq.gaffer.operation.impl.get.GetElements",
        "description": "Given your selected seeds, it gets related Entities and Edges.",
        "input": true,
        "view": true,
        "inOutFlag": true,
        "arrayOutput": true
      },
      {
        "name": "Get Adjacent IDs",
        "class": "uk.gov.gchq.gaffer.operation.impl.get.GetAdjacentIds",
        "description": "Given your selected seeds, it gets vertices at the other end of connected edges.",
        "input": true,
        "view": true,
        "inOutFlag": true,
        "arrayOutput": true
      },
      {
        "name": "Get Elements Between Sets",
        "class": "uk.gov.gchq.gaffer.accumulostore.operation.impl.GetElementsBetweenSets",
        "description": "Gets edges where the source and destination appear in both sets. As well as entities which appear in the first set",
        "input": true,
        "inputB": true,
        "arrayOutput": true,
        "view": true,
        "inOutFlag": true
      },
      {
        "name": "Get Elements Within Set",
        "class": "uk.gov.gchq.gaffer.accumulostore.operation.impl.GetElementsWithinSet",
        "description": "Retrieves edges where both ends are in a given set, or entities where the vertex is in the set",
        "input": true,
        "view": true,
        "inOutFlag": false,
        "arrayOutput": true
      },
      {
        "name": "Get Elements In Ranges",
        "class": "uk.gov.gchq.gaffer.accumulostore.operation.impl.GetElementsInRanges",
        "description": "Retreives all data between the seeds",
        "input": "uk.gov.gchq.gaffer.commonutil.pair.Pair",
        "view": true,
        "inOutFlag": true,
        "arrayOutput": true
      }
    ]
  },
  "graph": {
    "style": {
      "edges": {
        "RoadUse": {
          "line-color": "#9C27B0",
          "target-arrow-color": "#9C27B0",
          "text-outline-color": "#9C27B0"
        },
        "JunctionLocatedAt": { 
          "line-color": "#F44336",
          "target-arrow-color": "#F44336",
          "text-outline-color": "#F44336"
        },
        "RoadHasJunction": {
          "line-color": "#009688",
          "target-arrow-color": "#009688",
          "text-outline-color": "#009688"
        },
        "RegionContainsLocation": {
          "line-color": "#FF9800",
          "target-arrow-color": "#FF9800",
          "text-outline-color": "#FF9800"
        },
        "LocationContainsRoad": {
          "line-color": "#795548",
          "target-arrow-color": "#795548",
          "text-outline-color": "#795548"
        }
      },
      "vertexTypes": {
        "junction": {
          "background-image": "app/img/material-icons/call_split.svg",
          "text-valign": "top"
        },
        "road": {
          "background-image": "app/img/material-icons/directions_car.svg",
          "text-valign": "top"
        },
        "location": {
          "background-image": "app/img/material-icons/pin_drop.svg",
          "text-valign": "top"
        },
        "region": {
          "background-image": "app/img/material-icons/location_searching.svg",
          "text-valign": "top"
        },
        "coordinates": {
          "background-image": "app/img/material-icons/my_location.svg",
          "text-valign": "top"
        }
      }
    }
  },
  "time": {
    "filter": {
      "class": "java.util.Date",
      "unit": "millisecond",
      "startProperty": "startDate",
      "endProperty": "endDate"
    },
    "properties": {
      "startDate": {
        "class": "java.util.Date",
        "unit": "millisecond"
      },
      "endDate": {
        "class": "java.util.Date",
        "unit": "millisecond"
      }
    }
  }
}